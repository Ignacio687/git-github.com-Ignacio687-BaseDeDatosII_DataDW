                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        clasificacion.IDClasificacion, cliente.IDCliente, factura.IDFactura, detalleFactura.IDDetalleFac, producto.IDProducto, proveedor.IDProveedor, factura.Total, detalleFactura.Cantidad, factura.Fecha
FROM            factura INNER JOIN
                         cliente ON factura.IDCliente = cliente.IDCliente INNER JOIN
                         detalleFactura ON factura.IDFactura = detalleFactura.IDFactura INNER JOIN
                         producto ON detalleFactura.IDProducto = producto.IDProducto INNER JOIN
                         productosPorProveedor ON producto.IDProducto = productosPorProveedor.IDProducto INNER JOIN
                         proveedor ON productosPorProveedor.IDProveedor = proveedor.IDProveedor INNER JOIN
                         tipo ON producto.IDTipo = tipo.IDTipo INNER JOIN
                         clasificacion ON tipo.IDClasificacion = clasificacion.IDClasificacion</property>
